# Google Gemini API Key
# Get your API key from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Project Configuration
SUPABASE_PROJECT_ID=your_project_id_here

# Supabase Anon Key (get from Supabase Dashboard → Settings → API)
# This is the PUBLIC anon key - safe for client-side use
# Rotate this if it's been exposed publicly
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Vite environment variables (must be prefixed with VITE_ to be exposed to browser)
VITE_SUPABASE_URL=https://your_project_id_here.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
